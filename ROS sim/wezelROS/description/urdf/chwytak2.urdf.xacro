<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="chwytak">
<xacro:include filename="$(find wezelROS)/description/urdf/hand.transmission.xacro" />
<xacro:include filename="$(find wezelROS)/description/urdf/hand.gazebo.xacro" />
<xacro:macro name="hand" params="parent name side *origin">
  <link name="base_link2">
    <visual>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/base.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/base.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </collision>
    <inertial>
      <mass value="0.848"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
      <origin xyz="-0.000161 -0.049378 0.009251" rpy="1.57 0 3.14"/>
    </inertial>
  </link>

  <link name="rotator_link1">
    <visual>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger_rotator.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger_rotator.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
    </collision>
    <inertial>
      <mass value="0.011"/>
	<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
      <origin xyz="0 -0.011 0" rpy="1.57 0 0"/>
    </inertial>
  </link>

  <link name="rotator_link2">
    <visual>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger_rotator.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger_rotator.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </collision>
    <inertial>
      <mass value="0.011"/>
	<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
      <origin xyz="0 -0.011 0" rpy="1.57 0 3.14"/>
    </inertial>
  </link>

  <link name="rotator_link3">
    <visual>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger_rotator.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger_rotator.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </collision>
    <inertial>
      <mass value="0.011"/>
	<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
      <origin xyz="0 -0.011 0" rpy="1.57 0 3.14"/>
    </inertial>
  </link>

  <link name="finger_link1">
    <visual>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
    </collision>
    <inertial>
      <mass value="0.025"/>
	<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
      <origin xyz="0 0.028 -0.042" rpy="1.57 0 0"/>
    </inertial>
  </link>

  <link name="finger_link2">
    <visual>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </collision>
    <inertial>
      <mass value="0.025"/>
	<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
      <origin xyz="0 0.028 -0.042" rpy="1.57 0 3.14"/>
    </inertial>
  </link>

  <link name="finger_link3">
    <visual>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://wezelROS/description/meshes/finger.dae"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.57 0 3.14"/>
    </collision>
    <inertial>
      <mass value="0.025"/>
	<inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
      <origin xyz="0 0.028 -0.042" rpy="1.57 0 3.14"/>
    </inertial>
  </link>


  <joint name="joint_base" type="fixed">
        <parent link="${parent}" />
        <child link="base_link2" />
        <origin xyz="0.11 -0.008 -0.001" rpy="0 1.57 0" />
        <axis xyz="0 0 1" />
  </joint>

  <joint name="rotator1" type="fixed">
    <parent link="base_link2"/>
    <child link="rotator_link1"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0.04 0.021352"/>
  </joint>

  <joint name="rotator2" type="continuous">
    <parent link="base_link2"/>
    <child link="rotator_link2"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.026 -0.030397 0.021352" rpy="0 0 0.1185"/>
    <limit lower="-1.57" upper="1.57" effort="30" velocity="5"/>
  </joint>

  <joint name="rotator3" type="continuous">
    <parent link="base_link2"/>
    <child link="rotator_link3"/>
    <axis xyz="0 0 1"/>
    <origin xyz="-0.026 -0.030397 0.021352" rpy="0 0 -0.1185"/>
    <limit lower="-1.57" upper="1.57" effort="30" velocity="5"/>
    <mimic joint="rotator2" multiplier="-1"/>
  </joint>

  <joint name="finger1" type="revolute">
    <parent link="rotator_link1"/>
    <child link="finger_link1"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 0"/>
    <limit lower="-1" upper="1.57" effort="30" velocity="5"/>
  </joint>

  <joint name="finger2" type="revolute">
    <parent link="rotator_link2"/>
    <child link="finger_link2"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 0"/>
    <limit lower="-1.57" upper="1" effort="30" velocity="5"/>
  </joint>

  <joint name="finger3" type="revolute">
    <parent link="rotator_link3"/>
    <child link="finger_link3"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 0"/>
    <limit lower="-1.57" upper="1" effort="30" velocity="5"/>
  </joint>


   <xacro:hand_transmission name="${name}" />
    <xacro:hand_gazebo name="${name}" />
    
</xacro:macro>
</robot>
